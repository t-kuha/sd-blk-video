/include/ "system-conf.dtsi"
/ {
    /* SDSoC */
    xlnk {
        compatible = "xlnx,xlnk-1.0";
    };
};


&spi1 {
    status = "okay";
    num-cs = <1>;
    is-decoded-cs = <0>;
    #address-cells = <1>;
    #size-cells = <0>;

    dev@0 {
        compatible = "linux,spidev";
        reg = <0>;
        spi-max-frequency = <1000000>;
    };
};

&qspi {
        status = "okay";
        is-dual = <0>;
        num-cs = <1>;
        xlnx,qspi-mode = <1>;

        flash@0 {
                /* n25q128a: 16MiB */
                #address-cells = <1>;
                #size-cells =<1>;
                compatible = "jedec,spi-nor";
                reg = <0>;
                spi-max-frequency = <50000000>;
                m25p,fast-read;

                partition@bootloader {
                        /* BOOT.bin */
                        label = "bootloader";
                        reg = <0x0 0xe0000>;
                };
                partition@env {
                        label = "env";
                        reg = <0xe0000 0x20000>;
                };
                partition@image {
                        /* image.ub */
                        label = "image";
                        reg = <0x100000 0x500000>;
                };
        };
};
